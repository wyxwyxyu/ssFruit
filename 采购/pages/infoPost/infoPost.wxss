
.int{
  height:80rpx;
  line-height:80rpx;
}
.manager{
  display: flex;
  flex-direction: row;
  padding:20rpx 20rpx;
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  margin: 0 auto;
  color:#000;
  border-top: 4rpx solid #b0b4b4;
  border-bottom: 4rpx solid #b0b4b4;
}
.section{
  width:60%;
  height:80rpx;
  border:2rpx solid #000;
  line-height: 80rpx;
  text-align: center;
  color:#000;
  border-radius:8rpx;
  margin:10rpx 10rpx 0rpx 20rpx;
}

.text{
  width:30%;
  height: 80rpx;
  line-height: 100rpx; 
  text-align:center;
  border-radius:8rpx;
}


.button{
  width:90%;
  margin:40rpx auto;
  background: #0f7dde;
  color:#fff;
}
